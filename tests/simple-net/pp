(declare-fun s1 () Int)
(declare-fun s2 () Int)
(declare-fun s3 () Int)
(declare-fun t1 () Int)
(declare-fun t2 () Int)
(assert (>= s1 0))
(assert (>= s2 0))
(assert (>= s3 0))
(assert (>= t1 0))
(assert (>= t2 0))
(assert (= s1 (+ 1 (- t1) t2)))
(assert (= s2 (+ 0 t1 (- t2))))
(assert (= s3 (+ 0 t1 (- t2))))
(assert (or))
(check-sat)
(get-model)
